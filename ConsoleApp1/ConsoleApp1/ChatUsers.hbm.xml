<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="ConsoleApp1" namespace="ConsoleApp1">
  <class name="ChatUsers" table="ChatUsers" dynamic-update="true">
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="int">
      <generator class="native"/>
    </id>
    <property name="Userid"/>
    <property name="password"/>
    <property name="Status"/>
    <property name="Username"/>
  </class>
</hibernate-mapping>

